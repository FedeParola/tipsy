{
    "benchmark": [
        {
            "pipeline": {
                "name": "l3fwd",
                "core": 1,
                "fakedrop": true,
                "downlink-mac": "dd:dd:dd:dd:00:00",
                "downstream-group-table-size": 1,
                "downstream-l3-table-size": 2,
                "uplink-mac": "dd:dd:dd:dd:00:01",
                "upstream-group-table-size": 2,
                "upstream-l3-table-size": [10, 100, 1000, 10000]
            },
            "scale": "outer",
            "sut": {
                "bess-dir": "/opt/bess",
                "coremask": "0x3f03f",
                "downlink-port": "0000:00:08.0",
                "hostname": "sut.local",
                "portmask": "0x3",
                "tipsy-dir": "/opt/tipsy",
                "type": "ovs",
                "uplink-port": "0000:00:09.0"
            },
            "tester": {
                "type": "moongen-flood",
                "core": 1,
                "downlink-port": "1",
                "uplink-port": "0",
                "moongen-cmd": "/opt/MoonGen/build/MoonGen",
                "test-time": 30
            },
            "traffic": {
                "dir": "uplink",
                "pkt-num": 0,
                "pkt-size": 64,
                "random-seed": 1,
                "thread": 0
            }
        }
    ],
    "default": {
    },
    "visualize": [
        { "x-axis": "pipeline.upstream-l3-table-size",
          "y-axis": "out.flood.RX.PacketRate",
          "axis-type": "semilogx",
          "title": "{pipeline.name} - OVS v{out.sut.version}"
        },
        { "x-axis": "pipeline.upstream-l3-table-size",
          "y-axis": [
              "out.flood.RX.PacketRate",
              "out.flood.TX.PacketRate"
          ],
          "axis-type": "semilogx",
          "title": "{pipeline.name} - OVS v{out.sut.version}"
        }
    ]
}
